{
    "name": "wasserstein_vanilla_l3",
    "n_gpu": 1,
    "numpy_seed": 1,
    "torch_seed": 1,
    "wall_time": 8,
     "model": {
        "type": "WassersteinAutoEncoder",
        "args": {
            "latent_dimension": 3,
            "ksi": 0.5,
            "dropout": 0.1
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": false
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "log_dir": "logs/",
        "save_period": 10,
        "type": "Trainer",
        "options": {}
    },
    "data": {
        "dataset": {
            "data_dir": "/rap/jvb-000-aa/COURS2019/etudiants/data/horoma",
            "flattened": false
        },
        "dataloader": {
            "split": 0.9,
            "train": {
                "batch_size": 128,
                "shuffle": true
            },
            "valid": {
                "batch_size": 128,
                "shuffle": false
            }
        }
    }
}