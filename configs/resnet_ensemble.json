{
    "name": "resnet_dropout_ensemble",
    "n_gpu": 1,
    "numpy_seed": 1,
    "torch_seed": 1,
    "wall_time": 2,
    "model": {
        "type": "ResNet34_Ens",
        "args": {
            "num_classes": 17,
            "num_heads": 12,
            "dropout": 0.5
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0.0001,
            "amsgrad": false
        }
    },
    "trainer": {
        "epochs": 200,
        "save_dir": "saved/",
        "log_dir": "logs/",
        "save_period": 10,
        "type": "SupervisedTrainer",
        "options": {
            "WithEarlyStop":false,
            "n_clusters": 100,
            "kmeans_interval": 0,
            "kmeans_headstart": 0,
            "kmeans_weight": 10.0
        }
    },
    "data": {
        "dataset": {
            "data_dir": "/rap/jvb-000-aa/COURS2019/etudiants/submissions/b3phot2/data",
            "flattened": false
        },
        "dataloader": {
            "split": 0.6,
            "train": {
                "batch_size": 128,
                "shuffle": true
            },
            "valid": {
                "batch_size": 4,
                "shuffle": false
            }
        }
    }
}
