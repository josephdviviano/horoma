{
    "name": "vade_clusters_kmeans",
    "n_gpu": 1,
    "numpy_seed": 1,
    "torch_seed": 1,
    "wall_time": 8,
    "model": {
        "type": "VaDE",
        "args": {
            "input_dim": 10,
            "z_dim": 10,
            "n_centroids": 10,
            "binary": false,
            "encode_layers": [500,500,2000],
            "decode_layers": [2000,500,500],
            "loss_fct": "MSELoss"
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.0001,
            "weight_decay": 0,
            "amsgrad": false
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "log_dir": "logs/",
        "save_period": 10,
        "type": "ClusterKMeansVadeTrainer",
        "options": {
            "n_clusters": 100,
            "kmeans_interval": 0,
            "kmeans_headstart": 0,
            "kmeans_weight": 10.0
        }
    },
    "data": {
        "dataset": {
            "data_dir": "/rap/jvb-000-aa/COURS2019/etudiants/data/horoma",
            "flattened": false
        },
        "dataloader": {
            "split": 0.9,
            "train": {
                "batch_size": 128,
                "shuffle": true
            },
            "valid": {
                "batch_size": 128,
                "shuffle": false
            }
        }
    }
}
